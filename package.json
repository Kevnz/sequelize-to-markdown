    "extend": "^3.0.0",
    "jsdoc-api": "^3.0.0",
    "nunjucks": "^3.0.0",
    "sequelize": "^3.30.2",
    "sqlite3": "^3.1.8"
  },
  "devDependencies": {
    "jsdoc-to-markdown": "^3.0.0"
  }
