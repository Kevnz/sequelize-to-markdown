{
  "name": "sequelize-to-markdown",
  "version": "1.1.6",
  "description": "Create Markdown documentations out of sequelize models.",
  "main": "libs/",
  "scripts": {
    "docs": "node_modules/.bin/jsdoc2md libs/index.js > wiki/Home.md",
    "test": "./node_modules/.bin/nyc --reporter=lcov ./node_modules/.bin/mocha",
    "coverage": "npm run test && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OpusCapita/sequelize-to-markdown"
  },
  "bin": {
    "sq2md": "./index.js"
  },
  "preferGlobal": true,
  "keywords": [
    "sequelize",
    "markdown",
    "documentation"
  ],
  "author": "Christian Drengenberg <Christian.Drengenberg@opuscapita.com>",
  "license": "MIT",
  "dependencies": {
    "commander": "^2.9.0",
    "extend": "^3.0.0",
    "jsdoc-api": "^3.0.0",
    "nunjucks": "^3.0.0",
    "sequelize": "^3.30.2",
    "sqlite3": "^3.1.8"
  },
  "devDependencies": {
    "coveralls": "^2.12.0",
    "jsdoc-to-markdown": "^3.0.0",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2"
  }
}
